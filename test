passcheck = false
local niggaballkeys = {"123key123", "123anotherkey123"}
if table.find(niggaballkeys, getgenv().Key) then
passcheck = true
else
print("unwhitelisted")
game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("unwhitelisted", "All")
passcheck = false
end
repeat wait() until passcheck == true
print("whitelisted")
game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("whitelisted", "All")
